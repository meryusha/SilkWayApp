<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="silkway.merey.silkwayapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".agent.activities.MainActivity" />
        <activity
            android:name=".agent.activities.TourDetailsActivity"
            android:theme="@style/StyledIndicators" />
        <activity android:name=".agent.activities.TourProposalActivity" />
        <activity android:name=".agent.activities.TourCreateActivity" />
        <activity android:name=".agent.activities.TourEditActivity" />
        <activity android:name=".agent.activities.TourProposalEditActivity" />
        <activity android:name=".operator.activities.MainActivityOperator" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".agent.activities.TourProposalCreateActivity" />
        <activity android:name=".operator.activities.TourDetailsActivityOperator" />
        <!--
 ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information.
        -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity android:name=".agent.activities.TermsOfUseActivity" />
        <activity android:name=".agent.activities.EasyProposalActivity" />
        <activity android:name=".agent.activities.EasyProposalCreateActivity"></activity>
    </application>

</manifest>